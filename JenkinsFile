pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building Application...'
                echo 'Application built Successfully...'
            }
        }
        stage('Test') {
            steps {
                echo 'Running Tests...'
                echo 'Tests Executed Successfully...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying Application...'
                echo 'Application Deployed Successfully...'
            }
        }
    }
}
